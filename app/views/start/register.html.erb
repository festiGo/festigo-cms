<div class="content">

  <div id="wufoo-<%=Rails.configuration.wufoo_form_id%>">
    Fill out my <a href="https://festigo.wufoo.com/forms/<%=Rails.configuration.wufoo_form_id%>">online form</a>.
  </div>
  <div id="wuf-adv" style="font-family:inherit;font-size: small;color:#a7a7a7;text-align:center;display:block;">Use <a href="http://www.wufoo.com/partners/">Wufoo integrations</a> and get your data to your favorite apps.</div>
  <script type="text/javascript">var <%=Rails.configuration.wufoo_form_id%>;(function(d, t) {
    var s = d.createElement(t), options = {
      'userName':'festigo',
      'formHash':'<%=Rails.configuration.wufoo_form_id%>',
      'autoResize':true,
      'height':'638',
      'async':true,
      'host':'wufoo.com',
      'header':'show',
      'ssl':true};
    s.src = ('https:' == d.location.protocol ? 'https://' : 'http://') + 'wufoo.com/scripts/embed/form.js';
    s.onload = s.onreadystatechange = function() {
      var rs = this.readyState; if (rs) if (rs != 'complete') if (rs != 'loaded') return;
      try { <%=Rails.configuration.wufoo_form_id%> = new WufooForm();<%=Rails.configuration.wufoo_form_id%>.initialize(options);<%=Rails.configuration.wufoo_form_id%>.display(); } catch (e) {}};
    var scr = d.getElementsByTagName(t)[0], par = scr.parentNode; par.insertBefore(s, scr);
  })(document, 'script');</script>

</div>